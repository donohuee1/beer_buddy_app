<h1>Edit this user</h1>

<%= form_for @user do |f| %>

    <div>
        <%= f.label :user_name %>
        <%= f.text_field :user_name %>
    </div>

    <div>
        <%= f.label :email %>
        <%= f.text_field :email %>
    </div>

    <div>
        <%= f.label :age %>
        <%= f.text_field :age %>
    </div>

    <%= f.submit "Submit these edits" %>

<% end %>

<%= link_to "Back to user list!", users_path %>
