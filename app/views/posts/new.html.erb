<h1>Add a new post!</h1>

<%= form_for [@bar, @post] do |f| %>

    <!-- <div>
        <%#= f.label :bar_name %>
        <%#= f.text_field :bar_name %>
    </div> -->

    <div>
        <%= f.label :date %>
        <%= f.text_field :date %>
    </div>

    <div>
        <%= f.label :time %>
        <%= f.text_field :time %>
    </div>

    <div>
        <%= f.label :location %>
        <%= f.text_field :location %>
    </div>

    <div>
        <%= f.label :contents %>
        <%= f.text_field :contents %>
    </div>

    <%= f.submit "Add new post!" %>

<% end %>
